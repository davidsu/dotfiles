function calctabname() {
  nosrc=$(sed -E 's#.*(/[^/]+)#\1#') <<< $(sed -E 's#(.*)(/src)#\1#' <<< $(PWD))
  result=$(sed -E 's#.*(/[^/]+)#\1#')
  echo $result
  export PROMPT_COMMAND='echo -ne $(sed -E '\''s#.*(/[^/]+)#\1#'\'' <<< $(sed -E '\''s#(.*)(/src)#\1#'\'' <<< $(PWD)))':"$PROMPT_COMMAND";
}
if [ $ITERM_SESSION_ID ]; then
  calctabname
fi
